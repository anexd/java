package lab1;

import java.util.Scanner;
public class AsciiArt {
   
    public static void main(String[] args) {
          Scanner scan = new Scanner(System.in);
          System.out.print("");
          int quantidadeLinha = scan.nextInt();
          int i ;
          int k ;
          int j;
          int quantidadeColunas = quantidadeLinha ;
          for ( i = 0; i < quantidadeLinha; i++){ // para cada linha
            for ( k = i ; k < i; k++){
              // System.out.print(" ");  // imprime espaÃ§os
                
            }
            for ( j = 1; quantidadeColunas >= j; j++){
                System.out.print("*");// imprime asterisco
  
              if( k==1) {}
            	  else if(j==5) {
            		  System.out.print(" ");
            	  }
              

                }
           
           
           
           
          System.out.println(""); // pula linha 
}
                    
                   
        
          
          
          
       
        
    }
   
}