//package lab1;

import java.util.Scanner;
public class AsciiArt {
   
	public static void main(String[] args) {
 
        int i = 1;
	    int j;
	    int linha;
	    int coluna = 10;
	    Scanner scan = new Scanner(System.in);
        System.out.print("");
        linha = scan.nextInt();
        while(i<=linha){
            j=1;
            while(j<=coluna){
                if(j>=5&&i==2){
            	    while(j<=6&&i==2){
            	
                     System.out.print(" ");
                     j++;
                    }
                }
                if(j>=4&&i==3){
                    while(j<=7&&i==3){
                        
                        System.out.print(" ");
                        j++;
                    }
                    
                }
                if(j>=3&&i==4){
                    while(j<=8&&i==4){
                        System.out.print(" ");
                        j++;  
                    }
                }
                if(j>1&&i==5){
                    while(j<=9&&i==5){
                        System.out.print(" ");
                        j++;  
                    }
                }
                
                System.out.print("*");
                j++;
            }
            System.out.println(""); 
            i = i +1;
             
        }
              // System.out.print(" ");  // imprime espaÃ§os
                
    }         
} 

