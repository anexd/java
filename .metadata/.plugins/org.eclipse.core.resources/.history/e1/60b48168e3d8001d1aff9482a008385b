
public class Processador {
	String marca;
	String modelo;
	Double velocidade;
	int numNucleos;

	/*public  Processador(String marca,String modelo,Double velocidade, int numNucleos) {
		this.marca = marca;
		this.modelo = modelo;
		this.velocidade = velocidade;
		this.numNucleos = numNucleos;
