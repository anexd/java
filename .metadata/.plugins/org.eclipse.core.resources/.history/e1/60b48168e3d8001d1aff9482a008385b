	String getDescricao(){
		return("Processador: marca=,modelo=i7, velocidade=3.2GHz, numNucleos=8, velocidadeParalela=25.6GHz");
		 
	}
	Double getVelocidadeParalel(Double velocidade, int numNucleos) {
		return velocidade*numNucleos;
	}
	public static void  main(String[] arg) {
	 	Processador proc1= new Processador();
	 	proc1.marca ="Intel";
	 	proc1.modelo ="i7";
	 	proc1.velocidade = 3.2;
	 	proc1.numNucleos = 8;
	 	
	}
	
	
	
	
}
