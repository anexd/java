package lab1;

import java.util.Scanner;
public class SomaColecoes {
	public static void main(String[] args){
		int numero = 0;
		int acm = 0;
	
		do {
			Scanner scan = new Scanner(System.in);
			System.out.print("");
			numero = scan.nextInt();
			acm += numero;
			
		} while (numero==-1);
		System.out.println(acm);
	}
}
